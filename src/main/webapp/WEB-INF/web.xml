<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>CreateStudent</servlet-name>
		<servlet-class>com.prowings.controller.CreateStudentController</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>GetStudent</servlet-name>
		<servlet-class>com.prowings.controller.GetStudentController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CreateStudent</servlet-name>
		<url-pattern>/addStudent</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>GetStudent</servlet-name>
		<url-pattern>/getStudent</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>getStudent.jsp</welcome-file>
	</welcome-file-list>
</web-app>
